{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "sap.ui.demo.toolpageapp",
        "type": "application",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "_version": "1.1.0",
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_bluecrystal",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": {
            "viewName": "UI5FioriForTools.view.rootApp",
            "type": "XML",
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.30",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.tnt": {},
                "sap.ui.layout": {}
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlId": "mainContents",
                "viewType": "XML",
                "controlAggregation": "pages",
                "viewPath": "UI5FioriForTools.view",
                "async": true
            },
            "routes": [{
                    "pattern": "",
                    "name": "empty",
                    "target": "home"
                },
                {
                    "pattern": "home",
                    "name": "home",
                    "target": "home"
                },
                {
                    "pattern": "catalog",
                    "name": "catalog",
                    "target": "catalog"
                },
                {
                    "pattern": "ApiManagement",
                    "name": "ApiManagement",
                    "target": "ApiManagement"
                },
                {
                    "pattern": "CloudforCustomer",
                    "name": "CloudforCustomer",
                    "target": "CloudforCustomer"
                },
                {
                    "pattern": "CloudforFinance",
                    "name": "CloudforFinance",
                    "target": "CloudforFinance"
                },
                {
                    "pattern": "CloudPlatform",
                    "name": "CloudPlatform",
                    "target": "CloudPlatform"
                },
                {
                    "pattern": "GoalsManagement",
                    "name": "GoalsManagement",
                    "target": "GoalsManagement"
                },
                {
                    "pattern": "MarketingCloud",
                    "name": "MarketingCloud",
                    "target": "MarketingCloud"
                },
                {
                    "pattern": "ODataAPI",
                    "name": "ODataAPI",
                    "target": "ODataAPI"
                },
                {
                    "pattern": "ProductContent",
                    "name": "ProductContent",
                    "target": "ProductContent"
                },
                {
                    "pattern": "TalentManagement",
                    "name": "TalentManagement",
                    "target": "TalentManagement"
                },
                {
                    "pattern": "TimeRecordingAPI",
                    "name": "TimeRecordingAPI",
                    "target": "TimeRecordingAPI"
                },
                {
                    "pattern": "TranslationHub",
                    "name": "TranslationHub",
                    "target": "TranslationHub"
                },
                {
                    "pattern": "VehiclesNetwork",
                    "name": "VehiclesNetwork",
                    "target": "VehiclesNetwork"
                },
                {
                    "pattern": "ImportantLinks",
                    "name": "ImportantLinks",
                    "target": "ImportantLinks"
                }
            ],
            "targets": {
                "home": {
                    "viewName": "Home"
                },
                "catalog": {
                    "viewName": "Catalog"
                },
                "ApiManagement": {
                    "viewName": "catalog.ApiManagement"
                },
                "CloudforCustomer": {
                    "viewName": "catalog.CloudforCustomer"
                },
                "CloudforFinance": {
                    "viewName": "catalog.CloudforFinance"
                },
                "CloudPlatform": {
                    "viewName": "catalog.CloudPlatform"
                },
                "GoalsManagement": {
                    "viewName": "catalog.GoalsManagement"
                },
                "MarketingCloud": {
                    "viewName": "catalog.MarketingCloud"
                },
                "ODataAPI": {
                    "viewName": "catalog.ODataAPI"
                },
                "ProductContent": {
                    "viewName": "catalog.ProductContent"
                },
                "TalentManagement": {
                    "viewName": "catalog.TalentManagement"
                },
                "TimeRecordingAPI": {
                    "viewName": "catalog.TimeRecordingAPI"
                },
                "TranslationHub": {
                    "viewName": "catalog.TranslationHub"
                },
                "VehiclesNetwork": {
                    "viewName": "catalog.VehiclesNetwork"
                },
                "ImportantLinks": {
                    "viewName": "ImportantLinks"
                }
            }
        }
    }
}